{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "react/prop-types": 0,
    "no-bitwise": "off",
    "no-underscore-dangle": "off",
    "react/jsx-props-no-spreading": "off"
  },
  "env": {
    "browser": true,
    "jest": true
  }
}
